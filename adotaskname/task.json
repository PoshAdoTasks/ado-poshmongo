{
  "id": "776baf90-87c0-11ed-a863-93f72588df30",
  "name": "adotaskname",
  "friendlyName": "adotaskname",
  "description": "'An Azure Devops PowerShell Task'",
  "author": "Jeffrey Patton",
  "helpMarkDown": "Replace with markdown to show in help",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "minimumAgentVersion": "1.95.0",
  "instanceNameFormat": "adotaskname $(message)",
  "inputs": [
    {
      "name": "cwd",
      "type": "filePath",
      "label": "Working Directory",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": "Current working directory when adotaskname is run."
    },
    {
      "name": "msg",
      "type": "string",
      "label": "Message",
      "defaultValue": "Hello World",
      "required": true,
      "helpMarkDown": "Message to echo out"
    }
  ],
  "execution": {
    "PowerShell3": {
      "target": "sample.ps1"
    }
  }
}